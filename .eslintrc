{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "globals": {
    "fetch": true,
    "alert": true
  },
  "rules": {
    "no-use-before-define": 0,
    "react/no-did-mount-set-state": 0,
    "global-require": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0
  }
}
